<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="470.0" prefWidth="540.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="monopoly.controleur.ControleurAjoutJoueurs">
   <children>
      <TextField fx:id="textFieldNom" layoutX="21.0" layoutY="83.0" />
      <ComboBox fx:id="comboPions" layoutX="21.0" layoutY="125.0" prefWidth="150.0" />
      <Button fx:id="boutonAjouter" layoutX="21.0" layoutY="168.0" mnemonicParsing="false" onAction="#ajouterJoueur" prefHeight="25.0" prefWidth="149.0" text="Ajouter" />
      <TableView fx:id="tableauJoueurs" layoutX="224.0" layoutY="81.0" prefHeight="239.0" prefWidth="289.0">
        <columns>
          <TableColumn fx:id="colNom" prefWidth="153.0" text="Nom" />
          <TableColumn fx:id="colPion" prefWidth="134.0" text="Pion" />
        </columns>
      </TableView>
      <Label layoutX="21.0" layoutY="52.0" text="Ajouter un joueur" />
      <Label layoutX="224.0" layoutY="52.0" text="Liste des joueurs" />
      <Button fx:id="boutonRetirer" layoutX="224.0" layoutY="338.0" mnemonicParsing="false" onAction="#retirerJoueur" prefHeight="25.0" prefWidth="149.0" text="Retirer" />
      <Button fx:id="boutonLancerPartie" layoutX="146.0" layoutY="403.0" mnemonicParsing="false" onAction="#lancerPartie" prefHeight="25.0" prefWidth="249.0" text="Lancer la partie" />
   </children>
</AnchorPane>
